{
	"name": "cz-vinyl",
	"version": "1.3.2",
	"description": "Commitizen adapter formatting commit messages using Vinyl Depository conventions.",
	"author": "Vinyl Depository <dev@vinyldepository.com> (https://www.vinyldepository.com)",
	"keywords": [
		"commitizen",
		"adapter",
		"emoji",
		"jira"
	],
	"contributors": [
		{
			"name": "tal-rofe",
			"email": "tal@vinyldepository.com"
		}
	],
	"license": "MIT",
	"homepage": "https://github.com/Vinyl-Depository/cz-vinyl#readme",
	"bugs": {
		"url": "https://github.com/Vinyl-Depository/cz-vinyl/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Vinyl-Depository/cz-vinyl.git"
	},
	"main": "dist/index.js",
	"types": "dist/config.d.ts",
	"files": [
		"dist/**/*.js",
		"dist/config.d.ts"
	],
	"scripts": {
		"husky:install": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"setdev": "run-p husky:install git:config",
		"onboarding": "node ./scripts/onboarding.js",
		"prepare": "is-ci || run-s setdev onboarding",
		"dist": "webpack -c webpack.config.ts",
		"test": "vitest run -c vite.config.ts",
		"test:watch": "vitest watch -c vite.config.ts",
		"test:cov": "vitest run --coverage -c vite.config.ts",
		"lint": "eslint {src,tests}/**/*",
		"lint:fix": "eslint {src,tests}/**/* --fix",
		"format": "prettier --write \"**/*.{ts,js,json}\"",
		"type-check": "tsc --project ./tsconfig.json",
		"depcheck": "depcheck --config .depcheckrc.json",
		"inflint": "inflint -c inflint.config.ts"
	},
	"dependencies": {
		"cosmiconfig": "7.0.1",
		"cosmiconfig-typescript-loader": "1.0.9",
		"fuse.js": "6.6.1",
		"inquirer": "8.2.4",
		"inquirer-autocomplete-prompt": "2.0.0",
		"inquirer-maxlength-input-prompt": "1.0.2",
		"string-template": "1.0.0",
		"word-wrap": "1.2.3"
	},
	"devDependencies": {
		"@commitlint/cli": "16.2.4",
		"@types/inquirer": "8.2.1",
		"@types/inquirer-autocomplete-prompt": "1.3.4",
		"@types/json-diff": "0.7.0",
		"@types/node": "17.0.31",
		"@types/sinon": "10.0.11",
		"@types/string-template": "1.0.2",
		"@types/webpack-node-externals": "2.5.3",
		"@typescript-eslint/eslint-plugin": "5.22.0",
		"@typescript-eslint/parser": "5.22.0",
		"c8": "7.11.2",
		"chalk": "4.1.2",
		"commitizen": "4.2.4",
		"cz-vinyl": "1.3.2",
		"depcheck": "1.4.3",
		"eslint": "8.14.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"figlet": "1.5.2",
		"husky": "7.0.4",
		"inflint": "1.2.2",
		"is-ci": "3.0.1",
		"json-diff": "0.7.4",
		"lint-staged": "12.4.1",
		"npm-run-all": "4.1.5",
		"prettier": "2.6.2",
		"rimraf": "3.0.2",
		"sinon": "13.0.2",
		"ts-loader": "9.3.0",
		"ts-node": "10.7.0",
		"tsconfig-paths-webpack-plugin": "3.5.2",
		"typescript": "4.6.4",
		"vite": "2.9.8",
		"vite-tsconfig-paths": "3.4.1",
		"vitest": "0.10.5",
		"webpack": "5.72.0",
		"webpack-cli": "4.9.2",
		"webpack-node-externals": "3.0.0",
		"webpack-shell-plugin-next": "2.2.2"
	},
	"peerDependencies": {
		"@types/node": "17.0.31",
		"vite": "2.9.8"
	}
}
